{% extends 'base.html.twig' %}

{% block title %}Benificiaire index{% endblock %}

{% block body %}
    <h1>Benificiaire index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Relationfamiliale</th>
                <th>Nom</th>
                <th>Datenaissance</th>
                <th>Niveauetude</th>
                <th>Metier</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for benificiaire in benificiaires|reverse %}
            <tr>
                <td>{{ benificiaire.id }}</td>
                <td>{{ benificiaire.relationfamiliale }}</td>
                <td>{{ benificiaire.nom }}</td>
                <td>{{ benificiaire.datenaissance ? benificiaire.datenaissance|date('Y-m-d') : '' }}</td>
                <td>{{ benificiaire.niveauetude }}</td>
                <td>{{ benificiaire.metier }}</td>
                <td>
                    <a href="{{ path('app_benificiaire_show', {'id': benificiaire.id}) }}">show</a>
                    <a href="{{ path('app_benificiaire_edit', {'id': benificiaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_benificiaire_new') }}">Create new</a>
{% endblock %}
