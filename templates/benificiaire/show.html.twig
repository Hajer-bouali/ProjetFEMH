{% extends 'base.html.twig' %}

{% block title %}Benificiaire{% endblock %}

{% block body %}
    <h1>Benificiaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ benificiaire.id }}</td>
            </tr>
            <tr>
                <th>Relationfamiliale</th>
                <td>{{ benificiaire.relationfamiliale }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ benificiaire.nom }}</td>
            </tr>
            <tr>
                <th>Datenaissance</th>
                <td>{{ benificiaire.datenaissance ? benificiaire.datenaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Niveauetude</th>
                <td>{{ benificiaire.niveauetude }}</td>
            </tr>
            <tr>
                <th>Metier</th>
                <td>{{ benificiaire.metier }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_benificiaire_index') }}">back to list</a>

    <a href="{{ path('app_benificiaire_edit', {'id': benificiaire.id}) }}">edit</a>

    {{ include('benificiaire/_delete_form.html.twig') }}
{% endblock %}
