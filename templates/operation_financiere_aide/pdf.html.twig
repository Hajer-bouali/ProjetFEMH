						<label>
							<strong>date de creation :</strong>
						</label>
						
						{{ adherent.date  ? adherent.date|date('Y-m-d') : '' }}
				
			</div><br>

		</div>
	</body>
</html></div></div><!-- /profile info --></body></html>
